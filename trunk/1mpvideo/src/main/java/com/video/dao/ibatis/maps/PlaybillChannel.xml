<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="PlaybillChannel">
	<typeAlias alias="PlayBillChannel"
		type="com.video.domain.PlaybillChannel" />
	<resultMap id="PlayBillChannel" class="PlayBillChannel">
		<result property="id" column="ID" />
		<result property="name" column="NAME" />
		<result property="parent.id" column="parent_ID" />
		<result property="description" column="comm" />
		<result property="imageUrl" column="image_url" />
		<result property="grade" column="grade" />
		<result property="activeImageUrl" column="active_image_url" />
		<result property="deactivateImageUrl"
			column="deactivate_image_url" />
		<result property="wapName" column="wap_name" />
	</resultMap>
	<select id="getPlayBillChannels" resultMap="PlayBillChannel">
		select
		id,name,parent_id,comm,image_url,grade,active_image_url,deactivate_image_url,wap_name
		from w_playbill_channel
	</select>
	<select id="getPlaybillChannelById" parameterClass="java.lang.Long"
		resultMap="PlayBillChannel">
		select
		id,name,parent_id,comm,image_url,grade,active_image_url,deactivate_image_url,wap_name
		from w_playbill_channel where id=#id#;
	</select>
</sqlMap>